{
  "name": "ditto-quest-socket-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ditto-dao/ditto-quest-socket-server.git",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development dotenv -e .env ts-node ./index.ts",
    "build": "tsc",
    "pm2": "tsc && pm2 start pm2.config.json",
    "prisma:dev:migrate": "prisma migrate dev",
    "prisma:dev:generate": "prisma generate",
    "prisma:dev:studio": "prisma studio",
    "prisma:dev:push": "prisma db push",
    "prisma:prod:migrate": "prisma migrate deploy",
    "prisma:prod:generate": "prisma generate",
    "prisma:dev": "NODE_ENV=development yarn prisma:dev:generate && yarn prisma:dev:migrate",
    "prisma:prod": "NODE_ENV=production yarn prisma:prod:generate && yarn prisma:prod:migrate",
    "generate-items-from-csv": "ts-node ./src/encyclopedia/scripts/item-csv-to-json.ts",
    "generate-equipment-from-csv": "ts-node ./src/encyclopedia/scripts/equipment-csv-to-json.ts",
    "generate-traits-from-csv": "ts-node ./src/encyclopedia/scripts/traits-csv-to-json.ts",
    "generate-monsters-from-csv": "ts-node ./src/encyclopedia/scripts/monsters-csv-to-json.ts",
    "generate-domains-from-csv": "ts-node ./src/encyclopedia/scripts/domain-csv-to-json.ts",
    "generate-dungeons-from-csv": "ts-node ./src/encyclopedia/scripts/dungeon-csv-to-json.ts",
    "assign-random-stats-to-traits": "ts-node ./src/encyclopedia/scripts/assign-stats-to-traits.ts",
    "seed-items": "ts-node ./src/scripts/items/seed-items-db.ts",
    "seed-equipment": "ts-node ./src/scripts/equipment/seed-equipment-db.ts",
    "seed-crafting-recipes": "ts-node ./src/scripts/crafting/seed-crafting-recipes-db.ts",
    "seed-slime-traits": "ts-node ./src/scripts/slime-traits/seed-slime-traits-db.ts",
    "seed-monsters": "ts-node ./src/scripts/monsters/seed-monsters-db.ts",
    "seed-domains": "ts-node ./src/scripts/monsters/seed-domains-db.ts",
    "seed-dungeons": "ts-node ./src/scripts/monsters/seed-dungeons-db.ts",
    "purge-all-user-data": "ts-node ./src/scripts/purge-all-user-data-db.ts",
    "purge-tables": "ts-node ./src/scripts/purge-all-tables-db.ts",
    "trim-offline-activities": "ts-node ./src/scripts/redis-offline-activities/trim-user-offline-activities.ts",
    "breed": "ts-node ./src/utils/breed-test.ts",
    "test-exp-system": "ts-node ./src/scripts/simulation/exp-system-simulation.ts",
    "test-combat-system": "ts-node ./src/scripts/simulation/combat-simulation.ts",
    "process-test-eq-csv": "ts-node ./src/encyclopedia/scripts/process-test-eq-csv.ts",
    "generate-monster-stats": "ts-node ./src/encyclopedia/scripts/generate-monster-stats.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.730.0",
    "@aws-sdk/crc64-nvme-crt": "^3.730.0",
    "@aws-sdk/types": "^3.723.0",
    "@prisma/client": "^6.5.0",
    "async-lock": "^1.4.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "ethers": "^6.13.5",
    "express": "^4.21.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "prisma": "^6.5.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "csv-parser": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
